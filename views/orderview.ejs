                input: 'text',
                inputPlaceholder: 'Enter reason for cancellation',
                showCancelButton: true,
                confirmButtonText: 'Submit',
                cancelButtonText: 'Cancel',
                showLoaderOnConfirm: true,
                preConfirm: (reason) => {
                    return $.ajax({
                        url: `/cancelorder?id=${id}`, // Corrected URL interpolation
                        type: 'POST',
                        data: JSON.stringify({ id: id, reason: reason }), // Ensure data is stringified
                        contentType: 'application/json',
                    })
                    .then(response => {
                        if (response.error) { // Checking for error in response
                            alert(response.error);
                        } else {
                            Swal.fire('your order has been canceled', '', 'success');
                        //    setTimeout(function(){
                        //     window.location.reload();
                        //    },2000) 
                        }
                    })
                    .catch(error => {
                        Swal.showValidationMessage(`Request failed: ${error.statusText}`);
                    });
                },
                allowOutsideClick: () => !Swal.isLoading()
            });
        }
        
    
